  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "start": "npx nx serve dashboard --open",
    "serve:auth": "node server/server.js",
    "wizard": "npx nx serve wizard --port 4800 --open",
    "_portal": "-------------------------------------------------------------------------------",
    "serve:portal-app": "npx nx serve portal --open",
    "serve:features-api": "npx nx serve features-api",
    "serve:features-json": "json-server server/features.json --routes=server/routes.json --port=3000",
    "serve:portal-feature": "concurrently \"npm run serve:features-json\" \"npm run serve:portal-app\"",
    "_challenges": "-------------------------------------------------------------------------------",
    "serve:challenges-app": "npx nx serve challenges --open",
    "serve:challenges-api": "npx nx serve challenges-api",
    "serve:challenges-json": "json-server server/challenges.json --routes=server/routes.json --port=3100",
    "serve:challenges-feature": "concurrently \"npm run serve:challenges-json\" \"npm run serve:challenges-app\"",
    "_flashcards": "-------------------------------------------------------------------------------",
    "serve:flashcards-app": "npx nx serve flashcards --open",
    "serve:flashcards-api": "npx nx serve flashcards-api",
    "serve:flashcards-json": "json-server server/flashcards.json --routes=server/routes.json --port=3200",
    "serve:flashcards-feature": "concurrently \"npm run serve:flashcards-json\" \"npm run serve:flashcards-app\"",
    "_notes": "------------------------------------------------------------------------------------",
    "serve:notes-app": "npx nx serve notes --open",
    "serve:notes-api": "npx nx serve notes-api",
    "serve:notes-json": "json-server server/notes.json --routes=server/routes.json --port=3300",
    "serve:notes-feature": "concurrently \"npm run serve:notes-json\" \"npm run serve:notes-app\"",
    "_users": "------------------------------------------------------------------------------------",
    "serve:users-app": "npx nx serve users --open",
    "serve:users-api": "npx nx serve users-api",
    "serve:users-json": "json-server server/users.json --routes=server/routes.json --port=3400",
    "serve:users-feature": "concurrently \"npm run serve:users-json\" \"npm run serve:users-app\"",
    "_seeders": "----------------------------------------------------------------------------------",
    "seed:features": "ts-node remote/features/src/app/database/database.seeder.ts",
    "seed:challenges": "ts-node remote/challenges/src/app/database/database.seeder.ts",
    "seed:flashcards": "ts-node remote/flashcards/src/app/database/database.seeder.ts",
    "seed:notes": "ts-node remote/notes/src/app/database/database.seeder.ts",
    "seed:users": "ts-node remote/users/src/app/database/database.seeder.ts"
